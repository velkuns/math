{
  "name": "velkuns/math",
  "description": "Some common code for Puzzle game like Codingame or Advent Of Code",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Velkuns\\Math\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Velkuns\\Math\\Tests\\Unit\\": "./tests/unit",
      "Velkuns\\Math\\Tests\\Integration\\": "./tests/integration"
    }
  },

  "config": {
    "sort-packages": true
  },

  "require": {
    "php": "8.2.*||8.3.*"
  },

  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.41.0",
    "phpunit/phpcov": "^9.0.2",
    "phpunit/phpunit": "^10.5.2",
    "phpstan/phpstan": "^1.10.48",
    "phpstan/phpstan-phpunit": "^1.3.15"
  }
}
